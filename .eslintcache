   <span className=\"name\">\r\n                <Link to={{\r\n                                pathname: '/profile',\r\n                                state: {\r\n                                        id: suggestion._id.$oid\r\n                                }}} \r\n                        >                       \r\n                    {\r\n                        parts.map((part, index) => {\r\n                        const className = part.highlight ? 'highlight' : null;\r\n                        return (\r\n                                <span className={className}>{part.text}</span>                        \r\n                            );\r\n                        })\r\n                    }\r\n                </Link>\r\n            </span>\r\n          </span>\r\n        );\r\n      }\r\n     const onChange = (event, { newValue, method }) => {\r\n        setValue(newValue)\r\n      };\r\n      \r\n    const onSuggestionsFetchRequested = ({ value }) => {\r\n        setSuggestions(getSuggestions(value))\r\n      };\r\n    \r\n    const onSuggestionsClearRequested = () => {\r\n        setSuggestions([]);\r\n      };\r\n\r\n    const submitSearchData = async(e) => {\r\n        e.preventDefault();\r\n        let userData = allUsers.filter(user => user.name.toLowerCase() === value.toLowerCase());\r\n        console.log('User in search = ', userData)\r\n        userData = Object.assign({}, userData[0]);\r\n        if (userData) {\r\n            return(\r\n                <Link to={{\r\n                    pathname: '/profile',\r\n                    state: {\r\n                            id: userData._id.$oid\r\n                    }}} \r\n                > {value} </Link> \r\n            )\r\n        }\r\n    } \r\n    const inputProps = {\r\n        placeholder: \"Search for...\",\r\n        value,\r\n        onChange: onChange\r\n      };\r\n    return (\r\n        <Fragment>\r\n            <nav className=\"navbar navbar-expand navbar-light \r\n          